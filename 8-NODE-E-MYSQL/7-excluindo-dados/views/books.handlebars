<h1>Livros disponíveis</h1>
<div class="books-container">
  {{#each books}}
      <div>
        <span class="c-bold">#{{this.id}}</span> - 
        <span class="bold"><a href="/book/{{this.id}}">{{this.title}}</a></span> - 
        <span> {{this.pageqty}} páginas</span> 
      </div>
  {{/each}}
  <a href="/"><button class="form-btn" id="btn-book-page">Cadastrar livro</button></a>
</div>